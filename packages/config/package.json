{
  "name": "@bowdoin/config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./env": {
      "import": "./dist/env.js",
      "require": "./dist/env.cjs",
      "types": "./dist/env.d.ts"
    },
    "./flags": {
      "import": "./dist/flags.js",
      "require": "./dist/flags.cjs",
      "types": "./dist/flags.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "env": [
        "./dist/env.d.ts"
      ],
      "flags": [
        "./dist/flags.d.ts"
      ],
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "module": "./dist/index.js"
}
