{
  "name": "@bowdoin/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "prisma"
  ],
  "scripts": {
    "clean": "rimraf dist || rm -rf dist",
    "build": "pnpm run clean && pnpm run generate && tsup src/index.ts --format esm --dts --sourcemap --out-dir dist",
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset --force",
    "studio": "prisma studio",
    "seed": "tsx src/seed.ts",
    "prepare": "pnpm run generate",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0"
  },
  "dependencies": {
    "@bowdoin/config": "workspace:*",
    "@bowdoin/observability": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/client": "6.16.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "@vitest/ui": "2.1.9",
    "eslint": "^8.57.0",
    "prettier": "^3",
    "prisma": "6.16.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vitest": "2.1.9"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
